{
  "expo": {
    "name": "tranquillo",
    "slug": "tranquillo",
    "scheme": "ai.tranquillo",
    "deepLinking": true,
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#05665e"
    },
    "android": {
      "splash": {
        "image": "./assets/splash-icon.png",
        "resizeMode": "cover",
        "backgroundColor": "#05665e"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0C0E10"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.jeflo.tranquillo"
    },
    "ios": {
      "splash": {
        "image": "./assets/splash-icon.png",
        "resizeMode": "cover",
        "backgroundColor": "#004F4F"
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.jeflo.tranquillo",
      "associatedDomains": [
        "applinks:zcdffliliytbpivqkavn.supabase.co"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "favicon": "./assets/icon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser"
    ],
    "extra": {
      "EXPO_PUBLIC_APP_ENV": "production",
      "eas": {
        "projectId": "8cf26fea-5ada-4a4a-b81d-c39d8bbe9fd4"
      }
    }
  }
}
